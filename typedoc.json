{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["index.d.ts", "steamApiEnums.d.ts", "steamApiStructs.d.ts"],
  "entryPointStrategy": "Expand",
  "out": "wiki",
  "plugin": [
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-rename-defaults",
    "typedoc-plugin-markdown",
    "./utilities/jsdoc-theme.js"
  ],
  "theme": "github-wiki-custom",
  "name": "Steamworks",
  "includeVersion": true,
  "emit": "docs",
  "githubPages": true
}
